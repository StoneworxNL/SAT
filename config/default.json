{
    "checksFolder": "../qualityChecks/",
    "outputFolder": "./output",
    "SquatChecksFolder": "../SAT-Q/",
    "includeAppstore": false,
    "checks": [
        {
            "fnc": "NamingConvention",
            "squat": true,
            "options": {
                "allowedPrefixes": [
                    "ACO",
                    "ACT",
                    "ADE",
                    "ASU",
                    "BCO",
                    "BSD",
                    "CAL",
                    "CRS",
                    "CTL",
                    "CWS",
                    "CWS",
                    "DS",
                    "FNC",
                    "HCH",
                    "OCH",
                    "OEN",
                    "OLE",
                    "OPR",
                    "PRS",
                    "PWS",
                    "QUE",
                    "QUEUE",
                    "RET",
                    "RULE",
                    "SCE",
                    "SCH",
                    "SUB",
                    "TRN",
                    "VAL",
                    "WFA",
                    "WFC",
                    "WFS"
                ],
                "exceptionPrefixes": [
                    "ASU",
                    "BSD",
                    "CRS",
                    "CWS",
                    "HCH",
                    "VAL",
                    "WFA"
                ]
            }
        },
        {
            "fnc": "ErrorHandling",
            "squat": true,
            "options": {
                "allowedJava": [
                    "CommunityCommons.EndTransaction",
                    "CommunityCommons.getGUID",
                    "OIDC.DecodeJWTPlainText"
                ]
            }
        },
        {
            "fnc": "TooComplexMicroflow",
            "squat": true,
            "options": {
                "maxActions": 25,
                "maxComplexity": 30,
                "prefixExceptions": [
                    "VAL"
                ],
                "exceptionsMaxActions": 50,
                "exceptionsMaxComplexity": 100,
                "cxLoopCount": 4,
                "cxExclusiveSplit": 5,
                "cxObjectAction": 2,
                "cxMaxObjectExpression": 3,
                "cxVariableAction": 1,
                "cxMaxVariableExpression": 5
            }
        },
        {
            "fnc": "IllegalCommit",
            "squat": true,
            "options": {
                "allowedTopLevelPrefixes": [
                    "ACT",
                    "QUE",
                    "QUEUE",
                    "ACO",
                    "SCH"
                ],
                "allowedSubLevelPrefixes": [
                    "SUB"
                ]
            }
        },
        {
            "fnc": "IllegalShowPage",
            "squat": true,
            "options": {
                "allowedPrefixes": [
                    "ACT"
                ]
            }
        },
        {
            "fnc": "SplitMergeCheck",
            "squat": true
        },
        {
            "fnc": "MissingPermissions",
            "squat": true,
            "options": {
                "permissionPrefixes": [
                    "ACT",
                    "OCH",
                    "OEN",
                    "OLE",
                    "DS"
                ]
            }
        },
        {
            "fnc": "DemoUsers",
            "squat": true
        },
        {
            "fnc": "NestingTooDeep",
            "squat": true,
            "options": {
                "mxNesting": 5
            }
        },
        {
            "fnc": "DomainModel",
            "squat": true
        },
        {
            "fnc": "MenuStructure",
            "squat": true
        },
        {
            "fnc": "TopLevelFlows",
            "squat": true,
            "options": {
                "TopLevelPrefixes": [
                    "ACO",
                    "BCO",
                    "BCR",
                    "ACR",
                    "BDE",
                    "ADE",
                    "BRO",
                    "ARO",
                    "ACT",
                    "DS",
                    "OEN",
                    "OLE",
                    "OCH"
                ]
            }
        },
        {
            "fnc": "PageCommit",
            "squat": true
        },
        {
            "fnc": "IllegalObjectActions",
            "squat": true,
            "options": {
                "PrefixesNotAllowed": [
                    "DS",
                    "VAL"
                ]
            }
        },
        {
            "fnc": "GetOrCreateCheck",
            "squat": true,
            "options": {
                "allowedPrefixes": [
                    "SUB"
                ]
            }
        },
        {
            "fnc": "RestCall",
            "squat": true,
            "options": {
                "allowedPrefixes": [
                    "CRS"
                ]
            }
        }
    ]
}